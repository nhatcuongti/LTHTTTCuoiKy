
<body>
    <div class="container">
        <div class="card">

            <div class="card-body">
                <p class="text-center h2 mb-5">Danh sách đơn đặt hàng</p>
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th scope="col">Mã đơn hàng</th>
                            <th scope="col">Phí vận chuyển</th>
                        </tr>
                    </thead>
                    <tbody>
                    {{#each data}}
                        <tr>
                            <th scope="row">{{_id}}</th>
                            <td>20000</td>
                            <td>
                                <a class="btn btn-lg p-0  " href="/company/order/detail?id={{MaDH}}" role="button">
                                    <i class="fa fa-sign-in fa-2x" aria-hidden="true"></i>
                                </a>
                            </td>

                        </tr>
                    {{/each}}

                    </tbody>
                </table>
            </div>

        </div>
    </div>
